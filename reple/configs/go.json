{
    "run": "{bin_fname}",
    "compile" : "{compiler} {user_cargs} -o {bin_fname} {code_fname}",
    "compile_args": {
        "compiler": "go build",
        "code_suffix": ".go"
    },
    "template": "{template_begin}\n{prolog_lines}\n{template_main_begin}\n{repl_lines}\n{template_end}\n",
    "template_args": {
        "template_begin": "package main\nimport \"fmt\"\n",
        "template_main_begin": "func main() {",
        "template_end": "}",
        "line_epilogue": "",
        "output_processor" : {
            "type": "demarcated",
            "demarcater_template": "fmt.Print(\"{demarcater}\")",
            "command_symbol": "Â¶",
			"supported": {
				"prolog": false,
				"repl": true
			}
        }
    },
    "terminal_opts": {
        "lexer_fn": "GoLexer",
        "lexer_class": "pygments.lexers",
        "prolog_char": "$",
        "enclosers": [["{", "}"]]
    }
}
