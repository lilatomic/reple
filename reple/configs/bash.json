{
	"run": "bash {bin_fname}",
	"compile": "{compiler} {code_fname} {bin_fname}",
	"compile_args": {
		"compiler": "cp",
		"code_suffix": ".sh"
	},
	"template": "{template_begin}\n{prolog_lines}\n{repl_lines}\n",
	"template_args": {
		"template_begin": "#! /bin/bash",
		"line_epilogue": "",
		"output_processor": {
			"demarcater_template": "printf '\n{demarcater}\n'",
			"supported": {
				"prolog": true,
				"repl": true
			}
		}
	},
	"terminal_opts": {
		"lexer_fn": "BashLexer",
		"lexer_class": "pygments.lexers.shell",
		"prolog_char": "%",
		"enclosers": [
			[
				"(",
				")"
			],
			[
				"if",
				"fi"
			]
		]
	}
}
